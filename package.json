    "scaffold",
    "module",
    "node_module",
    "node-module",
    "tape",
    "tap",
    "coveralls",
    "coveralls.io",
    "travis",
    "javascript",
    "cli",
    "npm",
    "npm scripts",
    "productivity"
  ],
  "dependencies": {
    "camelcase": "*",
    "humanize-url": "*",
    "normalize-url": "*",
    "slug": "*",
    "yeoman-generator": "*",
    "yosay": "*"
  },
  "devDependencies": {
    "tape": "*",
